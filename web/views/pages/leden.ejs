<!DOCTYPE html>
<html>
<head>
    <title><%= text.page.members.title %></title>
    <% include ../partials/head %>
</head>
<body data-spy="scroll" data-target="#menu">
    <header>
        <% include ../partials/header %>
    </header> 
    <main>
        <section id="leden">
            <div class="container">
                <div class="row">
                    <div class="col-lg">
                        <h1><%= text.page.members.members_stura %></h1>
                        <p><%= text.page.members.desc %></p><br>
                        <% text.page.members.list.forEach(function(fac) { %>
                            <div class="row justify-content-center">
                                <h3><%= fac.name %></h3>
                            </div>
                            <div class="row">
                                <% fac.list.forEach(function(member) { %>
                                <div class="col-md member">
                                    <div class="row justify-content-center">
                                            <img src="<%= member.img %>" width="200px" alt="<%= member.img_alt %>">
                                    </div>
                                    <div class="row justify-content-center">
                                        <h5><%= member.name %></h5>
                                    </div>
                                    <div class="row justify-content-center">
                                        <p><%= member.desc %></p>
                                    </div>
                                </div>
                                <% }) %>
                            </div>
                        <% }) %>
                    </div>
                </div>
            </div>
        </section>   
    </main>
    <footer>
        <% include ../partials/footer %>
    </footer>        
</body>
</html>
